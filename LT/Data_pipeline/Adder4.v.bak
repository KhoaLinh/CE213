`include "Full_adder.v"
module Adder4(sum, carry, inA, inB);
	output [3:0]sum;
	output carry;
	input [3:0]inA, inB;
	
	Full_adder a0(sum[0],c0, inA[0],inB[0],1'b0);
	Full_adder a1(sum[1],c1, inA[0],inB[0], c2);
	FUll_adder a2(sum[2], c2, inA[0], inB[0], c3);
	Full_adder a3(sum[3], c3, inA[0], inB[0], carry);
	
endmodule
	